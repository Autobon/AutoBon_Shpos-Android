<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp45"
        android:background="@color/main_orange">

        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="@dimen/dp35"
            android:layout_height="match_parent"
            android:clickable="true"
            android:focusable="true"
            android:padding="@dimen/dp10"
            android:scaleType="centerInside"
            android:src="@mipmap/default_back" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/cooperative_title"
            android:textColor="@android:color/white"
            android:textSize="@dimen/sp18" />
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        android:layout_marginTop="@dimen/dp45"
        android:background="@color/main_white">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <ViewStub
                android:id="@+id/view_stub_failed_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout="@layout/view_stub_failed_status"/>
            <ViewStub
                android:id="@+id/view_stub_check_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout="@layout/view_stub_check_status"/>
            <ViewStub
                android:id="@+id/view_stub_success_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout="@layout/view_stub_success_status"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp30"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp20"
                android:gravity="center">
                <View
                    android:layout_width="@dimen/dp100"
                    android:layout_height="1dp"
                    android:background="@color/lightgray"
                    android:layout_marginLeft="@dimen/dp15"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/league_info_title"
                    android:layout_marginLeft="@dimen/dp15"
                    android:layout_marginRight="@dimen/dp15"
                    android:textSize="@dimen/sp15"/>
                <View
                    android:layout_width="@dimen/dp100"
                    android:layout_height="1dp"
                    android:background="@color/lightgray"
                    android:layout_marginRight="@dimen/dp15"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp10">
                <TextView
                    android:id="@+id/tv_company"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv_license_str"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"/>
                <TextView
                    android:id="@+id/tv_company_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_toRightOf="@id/tv_company"/>
                <TextView
                    android:id="@+id/tv_company_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv_license_num_str"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_below="@id/tv_company"
                    android:layout_marginTop="@dimen/dp10"/>
                <TextView
                    android:id="@+id/tv_company_num_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_toRightOf="@id/tv_company_num"
                    android:layout_below="@id/tv_company_value"
                    android:layout_marginTop="@dimen/dp10"/>
                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv_person_name_str"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_below="@id/tv_company_num"
                    android:layout_marginTop="@dimen/dp10"/>
                <TextView
                    android:id="@+id/tv_name_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_toRightOf="@id/tv_name"
                    android:layout_below="@id/tv_company_num_value"
                    android:layout_marginTop="@dimen/dp10"/>
                <TextView
                    android:id="@+id/tv_card_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv_card_number_str"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_below="@id/tv_name"
                    android:layout_marginTop="@dimen/dp10"/>
                <TextView
                    android:id="@+id/tv_card_number_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_toRightOf="@id/tv_card_number"
                    android:layout_below="@id/tv_name_value"
                    android:layout_marginTop="@dimen/dp10"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp30"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp10"
                android:gravity="center">
                <View
                    android:layout_width="@dimen/dp90"
                    android:layout_height="1dp"
                    android:background="@color/lightgray"
                    android:layout_marginLeft="@dimen/dp15"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv_license_copy_str"
                    android:layout_marginLeft="@dimen/dp10"
                    android:layout_marginRight="@dimen/dp10"
                    android:textSize="@dimen/sp15"/>
                <View
                    android:layout_width="@dimen/dp90"
                    android:layout_height="1dp"
                    android:background="@color/lightgray"
                    android:layout_marginRight="@dimen/dp15"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp10">
                <ImageView
                    android:id="@+id/iv_pic_one"
                    android:layout_width="@dimen/dp190"
                    android:layout_height="@dimen/dp120"
                    android:background="@color/lightgray"
                    android:layout_centerInParent="true"
                    android:scaleType="fitXY"/>
                <TextView
                    android:id="@+id/tv_one_example_tips"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/card_photo"
                    android:layout_centerInParent="true"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/line_color"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp30"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp10"
                android:gravity="center">
                <View
                    android:layout_width="@dimen/dp70"
                    android:layout_height="1dp"
                    android:background="@color/lightgray"
                    android:layout_marginLeft="@dimen/dp15"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv_card_photo_str"
                    android:layout_marginLeft="@dimen/dp15"
                    android:layout_marginRight="@dimen/dp15"
                    android:textSize="@dimen/sp15"/>
                <View
                    android:layout_width="@dimen/dp70"
                    android:layout_height="1dp"
                    android:background="@color/lightgray"
                    android:layout_marginRight="@dimen/dp15"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp10">
                <ImageView
                    android:id="@+id/iv_pic_two"
                    android:layout_width="@dimen/dp190"
                    android:layout_height="@dimen/dp120"
                    android:background="@color/lightgray"
                    android:layout_centerInParent="true"
                    android:scaleType="fitXY"/>
                <TextView
                    android:id="@+id/tv_two_example_tips"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/card_photo"
                    android:layout_centerInParent="true"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/line_color"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp30"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp10"
                android:gravity="center">
                <View
                    android:layout_width="@dimen/dp100"
                    android:layout_height="1dp"
                    android:background="@color/lightgray"
                    android:layout_marginLeft="@dimen/dp15"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/receipt_str"
                    android:layout_marginLeft="@dimen/dp15"
                    android:layout_marginRight="@dimen/dp15"
                    android:textSize="@dimen/sp15"/>
                <View
                    android:layout_width="@dimen/dp100"
                    android:layout_height="1dp"
                    android:background="@color/lightgray"
                    android:layout_marginRight="@dimen/dp15"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp10">
                <TextView
                    android:id="@+id/license_header_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/receipt_head_name"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"/>
                <TextView
                    android:id="@+id/license_header_name_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_toRightOf="@id/license_header_name"/>
                <TextView
                    android:id="@+id/tv_tax_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tax_number"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_below="@id/license_header_name"
                    android:layout_marginTop="@dimen/dp10"/>
                <TextView
                    android:id="@+id/tv_tax_name_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_toRightOf="@id/tv_tax_name"
                    android:layout_below="@id/license_header_name_value"
                    android:layout_marginTop="@dimen/dp10"/>
                <TextView
                    android:id="@+id/tv_mail_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mail_name"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_below="@id/tv_tax_name"
                    android:layout_marginTop="@dimen/dp10"/>
                <TextView
                    android:id="@+id/tv_mail_num_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_toRightOf="@id/tv_mail_num"
                    android:layout_below="@id/tv_tax_name_value"
                    android:layout_marginTop="@dimen/dp10"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/lightgray"
                android:layout_marginTop="@dimen/dp20"
                android:layout_marginLeft="@dimen/dp15"
                android:layout_marginRight="@dimen/dp15"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp10">
                <TextView
                    android:id="@+id/mail_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/receipt_head_name"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"/>
                <TextView
                    android:id="@+id/mail_address_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_toRightOf="@id/mail_address"/>
                <TextView
                    android:id="@+id/cooperative_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tax_number"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_below="@id/mail_address"
                    android:layout_marginTop="@dimen/dp10"/>
                <TextView
                    android:id="@+id/cooperative_address_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:layout_marginLeft="@dimen/dp15"
                    android:textSize="@dimen/sp15"
                    android:textColor="@color/darkgray"
                    android:layout_toRightOf="@id/cooperative_address"
                    android:layout_below="@id/mail_address_value"
                    android:layout_marginTop="@dimen/dp10"/>
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</merge>